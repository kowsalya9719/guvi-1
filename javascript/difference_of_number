#difference of number
const readline = require('readline');
const inp = readline.createInterface({input: process.stdin});
inp.on("line", (data) =>
{
   var a=[];
   a=data.split(" ");
   var b=a[0];
   var c=a[1];
   var s;
if(parseInt(b)<parseInt(c))
{
     s=parseInt(c)-parseInt(b);
    console.log(parseInt(s));
}
else
{
  s=parseInt(b)-parseInt(c);
  console.log(parseInt(s));
}
});
