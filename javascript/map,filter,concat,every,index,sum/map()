const readline=require('readline');
const inp=readline.createInterface({
    input:process.stdin
});
const a=[];
inp.on('line',(data)=>{
    a.push(data);
 });
var k=[];
var l=0;
var sum=0;
var res=[];
var g;
inp.on('close',()=>{
    k=a[0].split(" ");
function map()
{
 for(var i=0;i<k.length;i++)
 {  
     l=0;
     l=parseInt(k[i]*5);
     //console.log(l);
     res.push(l);
 }
 return g=res.join(" ");
 
}
res=map();
console.log(res);
});
//input
2 4 6
//output
10 20 30
