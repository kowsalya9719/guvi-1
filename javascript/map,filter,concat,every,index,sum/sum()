// SUM FUNCTION
const readline=require('readline');
const inp=readline.createInterface({
    input:process.stdin
});
const number=[];
inp.on('line',(data)=>{
    number.push(data);
 });
var a=[];
var sum=0;
inp.on('close',()=>{
    a=number[0].split(" ");
function add()
{
 for(var i=0;i<a.length;i++)
 {  
     sum=sum+parseInt(a[i]);
 }
 return sum;
 }
var result=add();
console.log(result);
});
//input
2 4 6
//outpu
12
